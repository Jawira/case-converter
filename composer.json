{
  "name": "jawira/case-converter",
  "type": "library",
  "description": "Convert string between Camel Case 🐪 and Snake Case 🐍",
  "keywords": [
    "Snake case",
    "Camel case"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jawira Portugal"
    }
  ],
  "require": {
    "php": ">=5.4",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "behat/behat": "^3.5"
  },
  "suggest": {
    "pds/skeleton": "PHP Package Development Standards"
  },
  "autoload": {
    "psr-4": {
      "Jawira\\CaseConverter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jawira\\CaseConverter\\Behat\\": "tests/behat/bootstrap/"
    }
  }
}

