{
  "name": "jawira/case-converter",
  "type": "library",
  "homepage": "https://jawira.github.io/case-converter/",
  "description": "Convert strings between 12 naming conventions: Snake case, Camel case, Pascal case, Kebab case, Ada case, Train case, Cobol case, Macro case, Upper case, Lower case, Sentence case, and Title case.",
  "keywords": [
    "Ada case",
    "Camel case",
    "Cobol case",
    "Kebab case",
    "Lower case",
    "Macro case",
    "Pascal case",
    "Sentence case",
    "Snake case",
    "Title case",
    "Train case",
    "Upper case"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jawira Portugal",
      "email": "dev@tugal.be",
      "homepage": "http://jawira.com/"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-mbstring": "*"
  },
  "suggest": {
    "pds/skeleton": "PHP Package Development Standards",
    "phing/phing": "PHP Build Tool"
  },
  "autoload": {
    "psr-4": {
      "Jawira\\CaseConverter\\": "src/"
    }
  }
}
